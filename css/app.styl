/* ----------------------------------------------
 nav
---------------------------------------------- */
.navbar-noround {
    border-radius: 0%;
}
#navbarcollapse{
    font-family: 'lato'; 
}
.navbar-noround {
    border-radius: 0px;
}
.logo {
    margin-top: 4px;
    display:block;
    width: 120px;
    height: 40px;
    background-image: url("logo.png");
    background-size: 120px 88px;
    background-position: 0px 40px;
    transition: background-position 0.5s;
}
.logo:hover{
    background-position: 0 0px;
}

