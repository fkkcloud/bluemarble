/* ----------------------------------------------
 slider
---------------------------------------------- */
.slide-wrap {
  margin: 5% auto 0;
  width: 70%;
}
.slide-mask {
  position: relative;
  overflow: hidden;
  height: 580px;
  width:  100%;
}
.slide-group {
  position: absolute; 
  top:  0px; 
  left: 0px;
  height: 100%;
  width: 100%;
}
.slide {
  height: 580px;
  width: 100%;
  font-family: "lato";
  font-size: 2em;
  color: #fff;
  text-align: center;
  list-style-type: none;
  background-color: #393939;
  display: block;
}

/* ----------------------------------------------
 slider
---------------------------------------------- */
.slide-nav {
  text-align: center;
}
.slide-nav ul {
    margin: 0 0 0 20px;
    padding: 0;
}
.slide-nav li {
  display: inline-block;
  width: 10px;
  height: 10px;
  background: rgb(175, 56, 56);
  cursor: pointer;
  margin-left: .5em;
}
.slide-nav li.current {
  background: rgb(144, 39, 39);
}
