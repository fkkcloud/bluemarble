@keyframes dash {
  to {
    stroke-dashoffset: 0;
  }
}

@keyframes strokeclr {
  15% {
    stroke: #1ebaff;
  }
  20% {
    stroke: #FF5050;
  }
  30% {
    stroke: #FF5050;
  }
  35% {
    stroke: #ff8c19;
  }
  45% {
    stroke: #ff8c19;
  }
  50% {
    stroke: #1ebaff;
  }
  99% {
    stroke: #1ebaff;
  }
  100% {
    stroke: #393939;
  }
}

@keyframes dashhide {
  55% {
    opacity: 0;
  }
  60% {
    opacity: 1;
  }
  99% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}

@keyframes deathhide {
  52% {
    border-radius: 50%;
  }
  57% {
    border-radius: 10%;
  }
  99% {
    border-radius: 10%;
  }
  100% {
    border-radius: 50%;
  }
}

