.lines {
  position: relative;
  display: inline;
  float:left;
  top: 105px;
  left: 138px;
  width: 100px;
  height: 100px;
  transform: scale(4.5);
}
.dash-lines {
  position: relative;
  display: inline;
  float:left;
  top: 105px;
  left: 138px;
  width: 100px;
  height: 100px;
  transform: scale(4.5);
  opacity: 0;
  margin-left: -100px;
}
.dash-lines-animation {
