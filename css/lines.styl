
/* ----------------------------------------------
 line - nodes
---------------------------------------------- */
.slide-lines{}

.start-node{
  position: relative;
  display: inline-block;
  float:left;
  top: 145px;
  left: 90px;
  width: 24px;
  height: 24px;
  background-color: #4AA02C;
  border-radius: 50%;
  z-index:10;
  border: 2px solid #FF5050;
  transform: scale(1.0);
}
.start-node-animation{
  -webkit-animation: nodestart 2.5s 100ms infinite ease-out;
}
.end-node{
  position: relative;
  display: inline-block;
  float:left;
  top: 145px;
  left: 340px;
  width: 24px;
  height: 24px;
  background-color: #C11B17;
  border-radius: 50%;
  z-index:10;
  border: 2px solid #ff8c19;
  transform: scale(1.0);
}
.end-node-animation{
  -webkit-animation: nodeend 2.5s 150ms infinite ease-out, deathhide 20s 100ms infinite ease-out; 
}
@-webkit-keyframes nodestart {
  0% {
    transform: scale(1.0);
    background-color: #4AA02C;
  }

  7% {
    background-color: #fff;
  }

  14% {
    transform: scale(1.4);
    background-color: #4AA02C;
  }

  21% {
    background-color: #fff;
  }

  28% {
    transform: scale(1.0);
    background-color: #4AA02C;
  }
}

@-webkit-keyframes nodeend {
  72% {
    transform: scale(1.0);
    background-color: #C11B17;
  }

  79% {
    background-color: #fff;
  }

  86% {
    transform: scale(1.4);
    background-color: #C11B17;
  }

  93% {
    background-color: #fff;
  }

  100% {
    transform: scale(1.0);
    background-color: #C11B17;
  }
}

